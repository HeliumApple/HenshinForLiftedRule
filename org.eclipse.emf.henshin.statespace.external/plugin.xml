<plugin>
   <extension
         point="org.eclipse.emf.henshin.statespace.validators">
      <validator
            class="org.eclipse.emf.henshin.statespace.external.cadp.CADPStateSpaceValidator"
            id="org.eclipse.emf.henshin.statespace.validator.cadp">
      </validator>
      <validator
            class="org.eclipse.emf.henshin.statespace.external.mcrl2.MCRL2StateSpaceValidator"
            id="org.eclipse.emf.henshin.statespace.validator.mcrl2">
      </validator>
      <validator
            class="org.eclipse.emf.henshin.statespace.external.prism.CTMCStateSpaceValidator"
            id="org.eclipse.emf.henshin.statespace.validator.prism.ctmc">
      </validator>
      <validator
            class="org.eclipse.emf.henshin.statespace.external.prism.CTMCSteadyStateTool"
            id="org.eclipse.emf.henshin.statespace.validator.prism.ctmc.steadystates">
      </validator>
      <validator
            class="org.eclipse.emf.henshin.statespace.external.prism.MDPStateSpaceValidator"
            id="org.eclipse.emf.henshin.statespace.validator.prism.mdp">
      </validator>
   </extension>
   <extension
         point="org.eclipse.emf.henshin.statespace.exporters">
      <exporter
            class="org.eclipse.emf.henshin.statespace.external.cadp.AUTStateSpaceExporter"
            id="org.eclipse.emf.henshin.statespace.external.aut">
      </exporter>
      <exporter
            class="org.eclipse.emf.henshin.statespace.external.tikz.TikZStateSpaceExporter"
            id="org.eclipse.emf.henshin.statespace.external.tikz">
      </exporter>
      <exporter
            class="org.eclipse.emf.henshin.statespace.external.prism.CTMCStateSpaceExporter"
            id="org.eclipse.emf.henshin.statespace.external.export.prismctmc">
      </exporter>
      <exporter
            class="org.eclipse.emf.henshin.statespace.external.prism.MDPStateSpaceExporter"
            id="org.eclipse.emf.henshin.statespace.external.export.prismmdp">
      </exporter>
   </extension>

</plugin>
